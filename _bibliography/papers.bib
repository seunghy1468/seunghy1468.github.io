@Article{2024_KSAS_spring,
  bibtex_show={true},
  abbr={KSAS},
  title="{저궤도 위성 네트워크 커버리지 분석 서베이}",
  author={Choi, E. and {\textbf{Yoo, S.}} and Kim, S. and Choi†, J. and Choi*, J.},
  abstract={위성 통신은 대량의 채널 용량과 함께 전 세계적인 통신 커버리지를 보장할 수 있는 잠재력을 가지고 있다. 보편적으로, 저궤도 위성 네트워크는 다수의 원형 궤도 하에 위성을 배치하는 모델로 설계되었다. 저궤도 위성은 정지궤도 위성에 비해 작은 커버리지로 인해 많은 수의 위성을 확보하는 것이 중요하다. 하지만 이러한 저궤도 위성 네트워크 모델의 커버리지 성능을 평가하기 위해서는 복잡한 수준의 시뮬레이션을 요구하므로 분석이 제한적이다. 따라서 최근에는, 푸아송 포인트 프로세스 (PPP)를 이용한 모델링을 이용해 위성 네트워크의 커버리지에 대해 분석하려는 시도가 있었다. 이러한 연구는 위성 네트워크에서의 위성 배치 전략에 대한 핵심 시스템 통찰력을 제공하였다. 커버리지에 대한 분석은 지상망과 위성망을 함께 고려하는 시스템으로도 확장되었으며, 더 확장된 시스템에서의 분석적 연구가 제안되었다. 본 논문에서는, 위성 네트워크에서 PPP를 이용한 커버리지 분석에 대해 진행된 연구에 대해 조사한다},
  journal={KSAS},
  pages={478--479},
  year={2024},
  url={https://www-dbpia-co-kr.libra.kaist.ac.kr/pdf/pdfView.do?nodeId=NODE11813729&width=1237},
}

